# Doing this because sbt-platform needs a well-formed git repository
$ exec git init .
$ exec git remote add origin https://github.com/scalaplatform/dummy
$ exec git fetch
$ exec git checkout -t origin/empty
$ exec git add .
$ exec git commit -m "test"

